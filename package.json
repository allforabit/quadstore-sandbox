{
  "name": "quadstore-sandbox",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.32",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node lib/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "yarn ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:debug": "yarn ts-node-dev --inspect --respawn --transpile-only src/index.ts",
    "cleardb": "yarn rimraf \"db/*\"",
    "generatedata": "yarn ts-node src/generate-data.ts",
    "clear-and-generate": "yarn cleardb && yarn generatedata",
    "run-query": "yarn ts-node src/query-list",
    "run-get": "yarn ts-node src/get-list"
  },
  "dependencies": {
    "@thi.ng/transducers": "^7.6.5",
    "@types/faker": "^5.1.7",
    "@types/leveldown": "^4.0.2",
    "@types/n3": "^1.4.4",
    "cross-env": "^7.0.3",
    "faker": "^5.4.0",
    "leveldown": "^5.6.0",
    "n3": "^1.8.1",
    "quadstore": "^8.0.0",
    "quadstore-comunica": "^0.3.1",
    "sparqljson-to-tree": "^2.1.0"
  }
}
